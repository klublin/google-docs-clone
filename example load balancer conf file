      server grading.cse356.compas.cs.stonybrook.edu:9000 max_fails=1 fail_timeout=1s;
      server grading.cse356.compas.cs.stonybrook.edu:9001 max_fails=1 fail_timeout=1s;
      server grading.cse356.compas.cs.stonybrook.edu:9002 max_fails=1 fail_timeout=1s;
   }
   server {
      listen 80;
      location / {
         proxy_next_upstream     error timeout invalid_header http_500;
          proxy_pass http://backend;
      }
   }
